{% extends "::base.html.twig" %}

{% block title %}Édition du domaine {{ domain.name }} - {{ parent() }}{% endblock %}

{% block body %}
<h1>Édition du domaine {{ domain.name }}</h1>
{{ form_start(form) }}
{{ form_rest(form) }}
<input type="submit" value="Éditer">
<a href="{{ path('portfolio_domain_show', { slug: domain.slug }) }}">Annuler</a>
{{ form_end(form) }}
{% endblock %}
